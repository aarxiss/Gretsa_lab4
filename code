#include <iostream>
using namespace std;

int main()
{
	setlocale(LC_ALL, "uk_UA");
	int arr[15] = { 1,2,3,3,4,4,5,6,7,8,9,10,11,12,12 };
	bool printed[15] = { false }; // масив, який відстежує,чи був елемент уже виведений
	cout << "Числа, які повторюються більше одного разу" << ": " << endl;
	for (int i = 0; i < 15; i++) { // цикл 'for',який проходить через всі індекси масиву з 15 елементів
		if (printed[i]) continue; // пропускає виведені елементи
		
		bool double1 = false; // дозволяє уникнути дублювання + забезпечує, що кожен повторюваний елемент буде виведений лише один раз.
		for (int j = i + 1; j < 15; j++) {
			if (arr[i] == arr[j]) {
				double1 = true;// змінна,що сигналізує,що знайдено повторення в масиві
				printed[j] = true;//фіксуємо, що цей елемент вже оброблено
			}
		}
		if (double1) { 
			cout << arr[i] << " " << endl;// виводимо елемент, якщо він має повторення
	   }
	}

	cout << endl;


}
